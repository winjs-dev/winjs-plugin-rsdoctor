{
  "name": "@winner-fed/plugin-rsdoctor",
  "description": "WinJS plugin for Rsdoctor - Build analysis and performance optimization tool",
  "keywords": [
    "winjs",
    "plugin",
    "rsdoctor",
    "winner-fed",
    "winjs-dev",
    "rspack",
    "build-analysis",
    "performance"
  ],
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/winjs-dev/winjs-plugin-rsdoctor.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build --watch",
    "lint": "biome check .",
    "lint:write": "biome check . --write",
    "prepare": "simple-git-hooks && npm run build",
    "test": "playwright test",
    "bump": "bumpp"
  },
  "simple-git-hooks": {
    "pre-commit": "npm run lint:write"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@playwright/test": "^1.55.1",
    "@rsbuild/core": "^1.5.13",
    "@rslib/core": "^0.18.0",
    "@types/node": "^24.0.0",
    "@winner-fed/winjs": "*",
    "bumpp": "10.3.1",
    "simple-git-hooks": "^2.13.1",
    "typescript": "^5.9.3"
  },
  "peerDevDependencies": {
    "@winner-fed/winjs": "*"
  },
  "peerDependencies": {
    "@winner-fed/utils": "*",
    "@rsdoctor/rspack-plugin": "^1.3.6",
    "@rsdoctor/webpack-plugin": "^1.3.6"
  },
  "peerDependenciesMeta": {
    "@rsbuild/core": {
      "optional": true
    },
    "@winner-fed/utils": {
      "optional": true
    },
    "@rsdoctor/rspack-plugin": {
      "optional": true
    },
    "@rsdoctor/webpack-plugin": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
